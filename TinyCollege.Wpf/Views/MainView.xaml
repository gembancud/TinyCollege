<views:MvxWpfView x:Class="TinyCollege.Wpf.Views.MainView"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:local="clr-namespace:TinyCollege.Wpf.Views"
                  xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
                  xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
                  xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
                  xmlns:xcad ="http://schemas.xceed.com/wpf/xaml/avalondock"
                  xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
                  xmlns:views1="clr-namespace:TinyCollege.Wpf.Views"
                  mc:Ignorable="d"
                  d:DesignHeight="600" d:DesignWidth="1000">
    <Grid>
        <xcad:DockingManager 
                           AllowMixedOrientation="True"
                           BorderBrush="Black"
                           BorderThickness="1"
                           GridSplitterWidth="9"
                           GridSplitterHeight="9"
            >
            <xcad:DockingManager.Theme>
                <xcad:MetroTheme />
            </xcad:DockingManager.Theme>
            <xcad:DockingManager.AnchorableHeaderTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="{Binding Title}" FontWeight="SemiBold"  />
                    </StackPanel>
                </DataTemplate>
            </xcad:DockingManager.AnchorableHeaderTemplate>
            <xcad:DockingManager.AnchorableTitleTemplate>
                <DataTemplate>
                    <GroupBox Header="{Binding Title}" Padding="0" Margin="0">
                    </GroupBox>
                </DataTemplate>
            </xcad:DockingManager.AnchorableTitleTemplate>
            <xcad:LayoutRoot x:Name="_layoutRoot">
                <xcad:LayoutPanel Orientation="Horizontal" DockHeight="*" DockWidth="*">
                    <xcad:LayoutPanel Orientation="Vertical" >
                        <xcad:LayoutAnchorablePaneGroup >
                            <xcad:LayoutAnchorablePane >
                                <xcad:LayoutAnchorable ContentId="tinycollege" Title="Tiny College" CanHide="False" CanClose="False"
                                            AutoHideWidth="250">

                                    <GroupBox Padding="5" Margin="0" BorderThickness="3">
                                        <GroupBox.HeaderTemplate>
                                            <DataTemplate>
                                                <iconPacks:PackIconModern Kind="Trophy"/>
                                            </DataTemplate>
                                        </GroupBox.HeaderTemplate>
                                        <ScrollViewer>
                                            <StackPanel>
                                                <Button Margin="2" Command="{Binding ViewSchoolsCommand}">Schools</Button>
                                                <Button Margin="2" Command="{Binding ViewCoursesCommand}">Courses</Button>
                                                <Button Margin="2" Command="{Binding ViewDepartmentsCommand}">Departments</Button>
                                                <Button Margin="2" Command="{Binding ViewProfessorsCommand}">Professors</Button>
                                                <Button Margin="2" Command="{Binding ViewStudentsCommand}">Students</Button>
                                            </StackPanel>
                                        </ScrollViewer>
                                    </GroupBox>
                                </xcad:LayoutAnchorable>
                            </xcad:LayoutAnchorablePane>
                        </xcad:LayoutAnchorablePaneGroup>
                        <xcad:LayoutAnchorablePaneGroup >
                            <xcad:LayoutAnchorablePane>
                                <xcad:LayoutAnchorable ContentId="motorpool" 
                                                   Title="Motor Pool" 
                                                   CanHide="False" 
                                                   CanClose="False"
                                                   AutoHideWidth="250">

                                    <GroupBox Padding="5" Margin="0" BorderThickness="3">
                                        <GroupBox.HeaderTemplate>
                                            <DataTemplate>
                                                <iconPacks:PackIconModern Kind="TransitCar"/>
                                            </DataTemplate>
                                        </GroupBox.HeaderTemplate>

                                        <ScrollViewer>
                                            <StackPanel>
                                                <Button Margin="2" Command="{Binding ViewEmployeesCommand}">Employees</Button>
                                                <Button Margin="2" Command="{Binding ViewPartsCommand}">Parts</Button>
                                                <Button Margin="2" Command="{Binding ViewMaintenancesCommand}">Maintenances</Button>
                                                <Button Margin="2" Command="{Binding ViewReservationsCommand}">Reservations</Button>
                                                <Button Margin="2" Command="{Binding ViewVehiclesCommand}">Vehicles</Button>
                                                <Button Margin="2" Command="{Binding ViewReportsCommand}">Reports</Button>
                                            </StackPanel>
                                        </ScrollViewer>
                                    </GroupBox>
                                </xcad:LayoutAnchorable>
                            </xcad:LayoutAnchorablePane>
                        </xcad:LayoutAnchorablePaneGroup>
                    </xcad:LayoutPanel>

                    <xcad:LayoutAnchorablePane>
                        <xcad:LayoutAnchorable ContentId="browser" 
                                               Title="Browser" 
                                               CanHide="False" 
                                               CanClose="False"
                                               AutoHideWidth="240">
                            <GroupBox Padding="5" Margin="0" BorderThickness="3" Header="{Binding }">
                                <GroupBox.HeaderTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <iconPacks:PackIconModern Kind="Eye"/>
                                            <TextBlock Text=" "></TextBlock>
                                            <TextBlock Text="{Binding BrowserHeader, Mode=OneWay}" ></TextBlock>
                                        </StackPanel>
                                    </DataTemplate>
                                </GroupBox.HeaderTemplate>
                                <DataGrid ItemsSource="{Binding BrowserEnumerable}" 
                                          SelectedItem="{Binding BrowserSelection, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                          AutoGenerateColumns="True" 
                                          ColumnWidth="*" 
                                          EnableColumnVirtualization="True" 
                                          EnableRowVirtualization="True"
                                          ScrollViewer.CanContentScroll="True" 
                                          ScrollViewer.VerticalScrollBarVisibility="Auto"
                                          ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                          IsReadOnly="True"
                                ></DataGrid>
                            </GroupBox>
                        </xcad:LayoutAnchorable>
                    </xcad:LayoutAnchorablePane>

                    <xcad:LayoutPanel Orientation="Vertical">
                        <xcad:LayoutAnchorablePaneGroup >
                            <xcad:LayoutAnchorablePane>
                                <xcad:LayoutAnchorable ContentId="inspector" 
                                                       Title="Inspector"  
                                                       CanHide="False" 
                                                       CanClose="False">
                                    <GroupBox Padding="5" Margin="0" BorderThickness="3" Header="{Binding}">
                                        <GroupBox.HeaderTemplate>
                                            <DataTemplate>
                                                <StackPanel Orientation="Horizontal">
                                                    <iconPacks:PackIconModern Kind="Magnify"/>
                                                    <TextBlock Text=" "></TextBlock>
                                                    <TextBlock Text="{Binding InspectorHeader, Mode=OneWay}" ></TextBlock>
                                                </StackPanel>
                                            </DataTemplate>
                                        </GroupBox.HeaderTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*"></RowDefinition>
                                                <RowDefinition Height="Auto"></RowDefinition>
                                            </Grid.RowDefinitions>
                                            <ScrollViewer>
                                                <StackPanel Orientation="Vertical">
                                                    <StackPanel Visibility="{Binding InspectorEmployeeVisibility, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="3*"></ColumnDefinition>
                                                                <ColumnDefinition Width="6*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Id</TextBlock>
                                                            <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorEmployee.EmployeeId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Name</TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorEmployee.Name}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3,8">Is Mechanic</TextBlock>
                                                            <CheckBox Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" IsChecked="{Binding InspectorEmployee.IsMechanic, Mode=TwoWay}" IsEnabled="False"></CheckBox>
                                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Reservation Forms</TextBlock>
                                                            <Button Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="100"  Command="{Binding InspectorEmployeeViewReservationFormsCommand}">View Forms</Button>
                                                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Maintenance Details</TextBlock>
                                                            <Button Grid.Row="4" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="100"  Command="{Binding InspectorEmployeeViewMaintenanceDetailsCommand}">View Maintenance Details</Button>

                                                        </Grid>
                                                    </StackPanel>
                                                    <StackPanel Visibility="{Binding InspectorMaintenanceVisibility, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="3*"></ColumnDefinition>
                                                                <ColumnDefinition Width="6*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Id</TextBlock>
                                                            <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenance.MaintenanceId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Type</TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenance.Type}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">CompletionDate</TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenance.CompletionDate, StringFormat=dd-MM-yyyy}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">ReleasingMechanicId</TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenance.ReleasingMechanicId}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">ReleasingMechanic</TextBlock>
                                                            <Button Grid.Row="4" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorMaintenanceReleasingMechanicCommand}">View ReleasingMechanic</Button>
                                                            <TextBlock Grid.Row="5" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">VehicleId</TextBlock>
                                                            <TextBlock Grid.Row="5" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenance.VehicleId}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="6" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Vehicle</TextBlock>
                                                            <Button Grid.Row="6" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorMaintenanceVehicleCommand}" >View Vehicle</Button>
                                                            <TextBlock Grid.Row="7" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">ReportId</TextBlock>
                                                            <TextBlock Grid.Row="7" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenance.ReportId}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="8" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Report</TextBlock>
                                                            <Button Grid.Row="8" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorMaintenanceReportCommand}" >View Report</Button>
                                                            <TextBlock Grid.Row="9" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8" >MaintenanceDetails</TextBlock>
                                                            <Button Grid.Row="9" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorMaintenanceMaintenanceDetailsCommand}">View MaintenanceDetails</Button>
                                                        </Grid>
                                                    </StackPanel>
                                                    <StackPanel Visibility="{Binding InspectorMaintenanceDetailVisibility, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="3*"></ColumnDefinition>
                                                                <ColumnDefinition Width="6*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Id</TextBlock>
                                                            <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenanceDetail.MaintenanceDetailId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">ProcessingDate</TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenanceDetail.ProcessingDate, StringFormat=dd-MM-yyyy}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">MaintenanceId</TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenanceDetail.MaintenanceId}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Maintenance</TextBlock>
                                                            <Button Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorMaintenanceDetailMaintenanceCommand}">View Maintenance</Button>
                                                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">PartUsages</TextBlock>
                                                            <Button Grid.Row="4" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorMaintenanceDetailPartUsagesCommand}">View PartUsages</Button>
                                                            <TextBlock Grid.Row="5" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">EmployeeId</TextBlock>
                                                            <TextBlock Grid.Row="5" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorMaintenanceDetail.EmployeeId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="6" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Employee</TextBlock>
                                                            <Button Grid.Row="6" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorMaintenanceDetailEmployeeCommand}">View Employee</Button>
                                                        </Grid>
                                                    </StackPanel>
                                                    <StackPanel Visibility="{Binding InspectorPartVisibility, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="3*"></ColumnDefinition>
                                                                <ColumnDefinition Width="6*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Id</TextBlock>
                                                            <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPart.PartId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Name</TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPart.Name}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">CurrentAmount</TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPart.CurrentAmount}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">MinimumLevel</TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPart.MinimumLevel}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8" >PartUsages</TextBlock>
                                                            <Button Grid.Row="4" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorPartPartUsagesCommand}">View PartUsages</Button>
                                                        </Grid>
                                                    </StackPanel>
                                                    <StackPanel Visibility="{Binding InspectorPartUsageVisibility, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="3*"></ColumnDefinition>
                                                                <ColumnDefinition Width="6*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Id</TextBlock>
                                                            <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPartUsage.PartUsageId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Count</TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPartUsage.Count}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">PartId</TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPartUsage.PartId}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Part</TextBlock>
                                                            <Button Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorPartUsagePartCommand}">View Part</Button>
                                                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">MaintenanceDetailId</TextBlock>
                                                            <TextBlock Grid.Row="4" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorPartUsage.MaintenanceDetailId}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="5" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">MaintenanceDetail</TextBlock>
                                                            <Button Grid.Row="5" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorPartUsageMaintenanceDetailCommand}">View MaintenanceDetail</Button>
                                                        </Grid>
                                                    </StackPanel>
                                                    <StackPanel Visibility="{Binding InspectorReportVisibility, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="3*"></ColumnDefinition>
                                                                <ColumnDefinition Width="6*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Id</TextBlock>
                                                            <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReport.ReportId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Month</TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReport.Month , StringFormat=MMMM}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Type</TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReport.Type}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Maintenances</TextBlock>
                                                            <Button Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorReportMaintenancesCommand}">View Maintenances</Button>
                                                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Reservations</TextBlock>
                                                            <Button Grid.Row="4" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" MaxWidth="200" Command="{Binding InspectorReportReservationsCommand}">View Reservations</Button>

                                                        </Grid>
                                                    </StackPanel>
                                                    <StackPanel Visibility="{Binding InspectorReservationVisibility, Converter={StaticResource BooleanToVisibilityConverter}}">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                                <RowDefinition Height="*"></RowDefinition>
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="3*"></ColumnDefinition>
                                                                <ColumnDefinition Width="6*"></ColumnDefinition>
                                                            </Grid.ColumnDefinitions>
                                                            <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Id</TextBlock>
                                                            <TextBlock Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReservation.ReservationId}" MinWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">DepartureDate</TextBlock>
                                                            <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReservation.DepartureDate , StringFormat=dd-MM-yyyy}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Destination</TextBlock>
                                                            <TextBlock Grid.Row="2" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReservation.Destination}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Billing</TextBlock>
                                                            <TextBlock Grid.Row="3" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReservation.Billing}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="4" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">Mileage</TextBlock>
                                                            <TextBlock Grid.Row="4" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" TextAlignment="Right" Text="{Binding InspectorReservation.Mileage}" MaxWidth="100" ></TextBlock>
                                                            <TextBlock Grid.Row="5" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="3, 8">ProfessorId</TextBlock>
